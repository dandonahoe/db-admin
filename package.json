{
  "name": "db-admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -rf node_modules src/web/node_modules src/api/node_modules && pnpm install && pnpm --prefix src/web install && pnpm --prefix src/api install && pnpm --prefix src/cli install",
    "dev:web": "pnpm --prefix src/web dev",
    "dev:api": "pnpm --prefix src/api run start",
    "build:api": "IS_BUILDING=true pnpm --prefix src/api build",
    "build:web": "IS_BUILDING=true pnpm --prefix src/web build",
    "analyze": "pnpm --prefix src/cli job:codebase",
    "cli": "ts-node ./src/cli/bin/dev.js",
    "cli:build": "pnpm --prefix src/cli build",
    "cli:lint": "pnpm --prefix src/cli lint",
    "cli:postpack": "pnpm --prefix src/cli postpack",
    "cli:posttest": "pnpm --prefix src/cli posttest",
    "cli:prepack": "pnpm --prefix src/cli prepack",
    "cli:test": "pnpm --prefix src/cli test",
    "cli:version": "pnpm --prefix src/cli version"
  },
